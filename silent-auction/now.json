{
    "version": 2,
    "name": "silent-auction",
    "builds": [
        {
            "src": "silent-auction/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "silent-auction/$1"
        }
    ]
}